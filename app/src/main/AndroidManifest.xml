<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fasttable">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fasttablelogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fasttablelogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.FastTable">
        <activity android:name=".ContactUs" ></activity>
        <activity android:name=".Receipt" />
        <activity android:name=".Invoice" />
        <activity android:name=".FileFeedback" />
        <activity android:name=".BookNow" />
        <activity android:name=".AboutUs" />
        <activity android:name=".UpdatePassword" />
        <activity android:name=".UpdateProfile" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".PasswordActivity" />
        <activity android:name=".Register" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Login"/>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>